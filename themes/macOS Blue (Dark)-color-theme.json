{
	"name": "macOS Blue (Dark)",
	"colors": {
		// 去除焦点边框
		"focusBorder": "#00000000",
		// 编辑器、标签页（激活）、面板、状态栏背景颜色
		"editor.background": "#232326",
		"tab.activeBackground": "#232326",
		"tab.activeBorder": "#232326",
		"panel.background": "#232326",
		"statusBar.background": "#232326",
		"tab.unfocusedActiveBorder": "#232326",
		// 侧栏、活动栏、标签页（非激活）背景颜色
		"activityBar.background": "#4f4f52",
		"sideBar.background": "#4f4f52",
		"editorGroupHeader.tabsBackground": "#4f4f52",
		"tab.inactiveBackground": "#38383a",
		// 活动栏非激活按钮颜色
		"activityBar.inactiveForeground": "#656565",
		// 按钮、菜单悬停和激活颜色
		"toolbar.hoverBackground": "#434445",
		"toolbar.activeBackground": "#434445",
		"list.inactiveSelectionBackground": "#68686b",
		"list.activeSelectionForeground": "#ffffff",
		"list.activeSelectionIconForeground": "#ffffff",
		// 边界线（非激活）
		"editorGroup.border": "#48484c",
		"editorGroupHeader.tabsBorder": "#48484c",
		"tab.border": "#48484c",
		"panel.border": "#48484c",
		// 主题色和次要主题色
		"selection.background": "#b3d7fe",
		"icon.foreground": "#007acc",
		"list.activeSelectionBackground": "#007acc",
		"list.focusOutline": "#007acc",
		"activityBar.foreground": "#007acc",
		"panelTitle.activeForeground": "#007acc",
		"editorLineNumber.activeForeground": "#007acc",
		// 文字和按钮
		"foreground": "#ebebeb",
		"editor.foreground": "#ebebeb",
		"button.foreground": "#404040",
		"panelTitle.inactiveForeground": "#ebebeb",
		"statusBar.foreground": "#ebebeb",
		"button.background": "#e7e7e9",
		"button.hoverBackground": "#ceced0",
		// 颜色设定为透明的组件
		"button.border": "#00000000",
		"button.separator": "#00000000",
		"list.hoverBackground": "#00000000",
		"sideBarTitle.foreground": "#00000000",
		"sideBarSectionHeader.background": "#00000000",
		"sideBarSectionHeader.border": "#00000000",
		"toolbar.hoverOutline": "#00000000",
		"panelTitle.activeBorder": "#00000000",
		"editorGroupHeader.border": "#00000000",
		// 编辑器数字和高亮
		"editorLineNumber.foreground": "#a2a2a5",
		"editor.lineHighlightBackground": "#303030",
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#546E7A"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#EEFFFF"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#F78C6C"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
